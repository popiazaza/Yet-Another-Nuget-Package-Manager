{
  "name": "yet-another-nuget-package-manager",
  "displayName": "Yet Another Nuget Package Manager",
  "description": "Yet Another Nuget Package Manager for Visual Studio Code",
  "version": "0.0.7",
  "publisher": "KaruhutKomol",
  "repository": {
    "type": "git",
    "url": "https://github.com/popiazaza/Yet-Another-Nuget-Package-Manager"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.csproj"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yet-another-nuget-package-manager.updatePackageInline",
        "title": "Update NuGet Package"
      },
      {
        "command": "yet-another-nuget-package-manager.openNugetPage",
        "title": "View on NuGet.org"
      },
      {
        "command": "yet-another-nuget-package-manager.selectPackageVersion",
        "title": "Select NuGet Package Version"
      },
      {
        "command": "yet-another-nuget-package-manager.searchAndAddPackage",
        "title": "Search and Add NuGet Package"
      },
      {
        "command": "yet-another-nuget-package-manager.showVulnerabilities",
        "title": "Show NuGet Package Vulnerabilities"
      },
      {
        "command": "yet-another-nuget-package-manager.upgradeAllPackages",
        "title": "Upgrade All NuGet Packages"
      },
      {
        "command": "yet-another-nuget-package-manager.removePackageInline",
        "title": "Remove NuGet Package"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "rimraf dist && npm run build:extension",
    "build:extension": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --minify",
    "watch": "npm run build:extension -- --watch",
    "pretest": "npm run build && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "@types/vscode": "~1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "esbuild": "^0.25.12",
    "fast-xml-parser": "^5.3.2"
  }
}
